	if ! test -f $@; then touch $@; fi
	cat $@ > $@.tmp
	list='$^'; for file in $$list; do \
	  echo $$file >> $@.tmp; \
	done
	cat $@.tmp | sort | uniq > $@
	rm $@.tmp

